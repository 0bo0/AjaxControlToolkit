    <AjaxMin SourceFiles="@(JS)" SourceExtensionPattern="\.js$" TargetExtension=".js" CollapseToLiteral="true" CombineDuplicateLiterals="true" LocalRenaming="CrunchAll" MacSafariQuirks="true" RemoveUnneededCode="true" StripDebugStatements="true" />
